server <- function(input, output) {
output$plot1 <- renderPlot({
plot(satDF$salary, satDF$sat)
})
output$plot2 <- renderPlot({
plot(satDF$expend, satDF$sat)
})
output$info_1 <- renderPrint({
nearPoints(satDF, input$plot_hover_1, xvar = "salary", yvar = "sat", threshold = 5)
})
output$info_2 <- renderPrint({
nearPoints(satDF, input$plot_hover_2, xvar = "expend", yvar = "sat", threshold = 5)
})
}
